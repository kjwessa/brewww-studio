    "@radix-ui/react-slot": "^1.0.2",
    "@sentry/nextjs": "^8.39.0",
    "@tailwindcss/typography": "^0.5.15",
    "babel-plugin-react-compiler": "19.0.0-beta-0dec889-20241115",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
